{
  "name": "sator-web-backend",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "tsc",
    "start": "nodemon",
    "migrate": "bunx prisma migrate dev",
    "push": "bunx prisma db push",
    "studio": "bunx prisma studio",
    "inspect": "nodemon --inspect src/index.ts",
    "reset-all": "bunx prisma db push --force-reset && bunx prisma db seed",
    "db-diff": "bunx prisma migrate diff --from-migrations prisma/migrations --to-schema-datamodel prisma/schema.prisma --shadow-database-url",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-pattern .gitignore",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts",
    "purge": "node --loader ts-node/esm ./prisma/purge.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prisma": "^6.4.0",
    "supertest": "^7.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.0.0",
    "@prisma/client": "^6.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/method-override": "^0.0.35",
    "@types/node": "^22.9.3",
    "@types/supertest": "^6.0.2",
    "@useoptic/express-middleware": "^0.0.5",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.20.1",
    "express": "^4.21.1",
    "globals": "^16.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "husky": "^9.1.7",
    "method-override": "^3.0.0",
    "nodemon": "^3.1.7",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.6",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  }
}
